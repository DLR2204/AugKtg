<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 		xmlns:context="http://www.springframework.org/schema/context" 
 		xsi:schemaLocation="http://www.springframework.org/schema/beans
 		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
 		http://www.springframework.org/schema/context
 		http://www.springframework.org/schema/context/spring-context-3.0.xsd"> 


		<!-- Employee emp = new Employee(); -->
	<bean id="emp" class="com.Employee">
	
	<constructor-arg name="empId" value="100"></constructor-arg>
	
	<constructor-arg name="empName" value="Sasank"></constructor-arg>
	
	<constructor-arg name="empSal" value="233453452"></constructor-arg>
	
	<constructor-arg name="empAdd" value="India"></constructor-arg>
	
	
	</bean>
	
	
	
		

</beans>